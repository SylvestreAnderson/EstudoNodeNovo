<h1>Usu√°rios Cadastrados:</h1>
<div class="container">
    {{#each users}}
    <div class="item">
        <h2>{{this.name}}</h2>
        <p>{{this.occupation}}</p>
        <a href="/users/{{this.id}}">Ver detalhes</a>
        <form action="/users/delete/{{this.id}}" class="details-btn" method="POST">
            <input type="submit" value="Excluir" class="delete-btn">
        </form>
    </div>
    {{/each}}
</div>
